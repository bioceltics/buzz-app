{
  "expo": {
    "name": "Buzzee for Business",
    "slug": "buzzee-venue",
    "scheme": "buzzee-venue",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#E91E63"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.buzzee.venue",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow camera access to scan customer QR codes for deal redemption verification."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E91E63"
      },
      "package": "com.buzzee.venue",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow camera access to scan customer QR codes."
        }
      ],
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": false,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d0647af2-88c1-4e39-bb9a-bc8c3b7570dd"
      }
    },
    "owner": "bioceltics"
  }
}
